{

  "name" : "thadafinser/es-index-switcher",
  "description" : "Switch your index, without search downtime",
  "type": "library",
  "keywords": [
    "elasticsearch",
    "elastic",
    "alias",
    "index",
    "reindex",
    "re-index"
  ],
  
  "require" : {
    "php" : "~7.0",
    
    "elasticsearch/elasticsearch": "^5.1"
  },
  
  "require-dev" : {
    "phpunit/phpunit" : "^6.0.7",
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  
  "license" : "MIT",
  
  "autoload" : {
    "psr-4" : {
      "EsIndexSwitcher\\" : "src"
    }
  },
  
  "autoload-dev" : {
    "psr-4" : {
      "EsIndexSwitcherTest\\" : "tests"
    }
  },
  
  "authors" : [ 
    {
        "name" : "ThaDafinser",
        "email" : "martin.keckeis1@gmail.com"
    }
  ]
}
